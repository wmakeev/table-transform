{
  "name": "@wmakeev/table-transform",
  "version": "9.0.0-15",
  "type": "module",
  "description": "Streaming table data transformation",
  "keywords": [
    "csv",
    "table",
    "convert",
    "transform",
    "stream",
    "etl"
  ],
  "exports": "./build/src/index.js",
  "main": "build/src/index.js",
  "types": "build/src/index.d.ts",
  "files": [
    "build/src/"
  ],
  "scripts": {
    "lint": "eslint . --fix",
    "cleanup": "rm -rf build/",
    "compile:dev": "tsc",
    "compile": "tsc --build tsconfig.prod.json",
    "build:dev": "npm run lint && npm run compile:dev",
    "build": "npm run cleanup && npm run lint && npm run compile",
    "test": "npm run build && node --env-file=.env --test \"build/**/*.test.js\" --test-reporter=spec",
    "version": "auto-changelog -p && git add CHANGELOG.md",
    "release": "np"
  },
  "engines": {
    "node": ">=18"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "quoteProps": "consistent",
    "trailingComma": "none",
    "arrowParens": "avoid"
  },
  "np": {
    "yarn": false
  },
  "author": "Vitaliy V. Makeev",
  "repository": {
    "type": "git",
    "url": "https://github.com/wmakeev/table-transform"
  },
  "license": "ISC",
  "devDependencies": {
    "@tsconfig/node18": "^18.2.4",
    "@tsconfig/strictest": "^2.0.5",
    "@types/node": "^22",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@typescript-eslint/typescript-estree": "^6.10.0",
    "@wmakeev/filtrex": "^6.4.0",
    "auto-changelog": "^2.4.0",
    "csv-parse": "^5.5.6",
    "csv-stringify": "^6.5.1",
    "decode-html": "^2.0.0",
    "envalid": "^8.0.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "lodash": "^4.17.21",
    "prettier": "^3.3.3",
    "simplex-lang": "^0.3.0",
    "typescript": "^5.5.4",
    "typescript-eslint-language-service": "^5.0.5"
  }
}
